<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox fx:id="menuLayout" alignment="CENTER" spacing="20"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.controller.MainController"
      stylesheets="@../css/mainmenu.css"
      prefWidth="400" prefHeight="300">

    <Button fx:id="playButton" text="Play"/>
    <Button fx:id="configButton" text="Configuration"/>
    <Button fx:id="highScoresButton" text="High Scores"/>
    <Button fx:id="exitButton" text="Exit"/>

</VBox>

